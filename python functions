        "balanec = 0\n",
        "x = 1234\n",
        "n = int(input(\"enter the number\"))\n",
        "if x==n:\n",
        "  print(\"WELLCOME TO ATM MECHINE\")\n",
        "  while True:\n",
        "    print('''  1)check balanec\n",
        "  2)withdarwal\n",
        "  3)deposit\n",
        "  4)exit ''')\n",
        "    optional = int(input(\"enter the number\"))\n",
        "    if optional == 1:\n",
        "      print(\"you current balance is\",balanec)\n",
        "    elif optional ==2:\n",
        "      withdarwal = int(input(\"enter the amount\"))\n",
        "      if withdarwal <= balanec:\n",
        "        balanec = balanec-withdarwal\n",
        "        print(\"you\\'s withdarwal is susscfull\")\n",
        "        print(\"remaing balanec\",balanec)\n",
        "      else:\n",
        "        print(\"insufficient balanec\")\n",
        "    elif optional == 3:\n",
        "      deposit = int(input(\"enter the amount\"))\n",
        "      balanec = balanec+deposit\n",
        "      print(\"deposit is susscessfull\")\n",
        "      print(\"your current balanec is\",balanec)\n",
        "    elif optional == 4:\n",
        "      print(\"exit\")\n",
        "      break;\n",
        "    print(\"THANK YOU FOR CHOUSEING HDFC BANK\")\n",
        "else:\n",
        "  print(\"INVALIED NUMBER \")\n",
        "print(\"THANK YOU FOR CHOUSEING HDFC BANK\")"
      ]
    },
