        "nums1 = [1,2,3,0,0,0]\n",
        "m = 3\n",
        "nums2 = [2,5,6]\n",
        "n = 3\n",
        "i = m - 1\n",
        "j = n - 1\n",
        "k = m + n - 1\n",
        "\n",
        "while j >= 0:\n",
        "    if i >= 0 and nums1[i] > nums2[j]:\n",
        "        nums1[k] = nums1[i]\n",
        "        i -= 1\n",
        "    else:\n",
        "        nums1[k] = nums2[j]\n",
        "        j -= 1\n",
        "    k -= 1\n",
        "\n",
        "print(nums1)\n"
